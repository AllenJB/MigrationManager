{
  "name": "allenjb/migrationmanager",
  "type": "library",
  "description": "A simple, framework agnostic, MySQL non-reversible migrations library that accepts a pre-configured PDO connection",
  "keywords": [
    "AllenJB",
    "MigrationManager",
    "PDO",
    "migration"
  ],
  "homepage": "https://github.com/AllenJB/MigrationManager",
  "license": "MIT",
  "authors": [
    {
      "name": "AllenJB",
      "email": "php-packages@allenjb.me.uk",
      "homepage": "https://github.com/AllenJB",
      "role": "Developer"
    }
  ],
  "require": {
    "php" : "^7.4|^8.0|^8.1"
  },
  "autoload": {
    "psr-4": {
      "AllenJB\\MigrationManager\\": "src"
    }
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
    "php-parallel-lint/php-parallel-lint": "^1.3",
    "phpcompatibility/php-compatibility": "^9.3",
    "phpstan/phpstan": "^1.9",
    "phpstan/phpstan-strict-rules": "^1.4",
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
